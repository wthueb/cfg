[user]
    name = William Huebner
    email = wilhueb@gmail.com
    username = wthueb

[core]
    editor = vim
    whitespace = fix,-indent-with-non-tab,tab-in-indent,tabwidth=4
    excludesfile = ~/.gitignore_global
    symlinks = true
    pager = delta

[delta]
    syntax-theme = Dracula

[web]
    browser = chrome

[difftool]
    prompt = false

[alias]
    a = add --all
    b = branch -v
    ba = branch -a
    bd = branch -d
    bdd = branch -D
    br = branch -r
    bc = rev-parse HEAD
    c = commit
    ca = commit -a
    cm = commit -m
    cam = commit -am
    cd = commit --amend
    cad = commit -a --amend
    ch = checkout
    chm = checkout master
    chb = checkout -b
    cl = clone
    d = diff
    dt = difftool
    f = fetch
    fo = fetch origin
    fu = fetch upstream
    g = grep -p
    l = log
    lo = log --oneline
    lg = log --oneline --graph --decorate
    ls = ls-files
    m = merge
    ma = merge --abort
    mc = merge --continue
    ms = merge --skip
    ps = push
    psf = push -f
    psu = push -u
    pso = push origin
    psao = push --all origin
    psfo = push -f origin
    psuo = push -u origin
    psom = push origin master
    psaom = push --all origin master
    psfom = push -f origin master
    psuon = push -u origin master
    pl = pull
    pb = pull --rebase
    plo = pull origin
    pbo = pull --rebase origin
    plom = pull origin master
    pbom = pull --rebase origin master
    plu = pull upstream
    plum = pull upstream master
    pbum = pull --rebase upstream master
    rb = rebase
    rba = rebase --abort
    rbc = rebase --continue
    rbi = rebase --interactive
    rbs = rebase --skip
    re = reset
    rh = reset HEAD
    reh = reset --hard
    rem = reset --mixed
    res = reset --soft
    rehh = reset --hard HEAD
    remh = reset --mixed HEAD
    resh = reset --soft HEAD
    r = remote
    ra = remote add
    rr = remote rm
    rv = remote -v
    rn = remote rename
    rs = remote show
    rao = remote add origin
    rau = remote add upstream
    rro = remote remove origin
    rru = remote remove upstream
    rso = remote show origin
    rsu = remote show upstream
    rmf = rm -f
    rmrf = rm -r -f
    s = status
    sb = status -s -b
    subup = submodule update --init --recursive
    subpull = !git submodule foreach git pull --tags origin master
[pull]
	rebase = false
[init]
	defaultBranch = master
